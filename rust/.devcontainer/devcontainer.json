{
    "build": {
        "dockerfile": "../Dockerfile",
        "context" : "..",
        "target": "base"
    },
    "extensions": [
        "rust-lang.rust"
    ],
    "workspaceMount": "source=${localWorkspaceFolder}/..,target=/usr/src/myapp,type=bind,consistency=cached",
    "workspaceFolder": "/usr/src/myapp/rust"
}